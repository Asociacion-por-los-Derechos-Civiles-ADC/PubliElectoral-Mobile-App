{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "android",
      "request": "launch",
      "name": "Android launch",
      "appSrcRoot": "${workspaceRoot}/app/src/main",
      "apkFile": "${workspaceRoot}/app/build/outputs/apk/debug/Frost-debug.apk",
      "adbPort": 5037
    },
    {
      "request": "launch",
      "type": "android",
      "name": "App Build & Launch",
      "appSrcRoot": "${workspaceRoot}/app/src/main",
      "apkFile": "${workspaceRoot}/app/build/outputs/apk/debug/Frost-debug.apk",
      "preLaunchTask": "run gradle"
    },
    {
      "type": "android",
      "request": "attach",
      "name": "Android attach",
      "appSrcRoot": "${workspaceRoot}/app/src/main",
      "adbPort": 5037,
      "processId": "${command:PickAndroidProcess}"
    },
    {
      "request": "launch",
      "type": "android",
      "name": "App Launch Release",
      "appSrcRoot": "${workspaceRoot}/app/src/main",
      "apkFile": "${workspaceRoot}/app/build/outputs/apk/release/Frost-release.apk",
      "preLaunchTask": "run gradle"
    }
  ]
}
